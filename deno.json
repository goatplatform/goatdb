{
  "compilerOptions": {
    "lib": ["dom", "dom.iterable", "dom.asynciterable", "deno.ns"],
    "jsx": "precompile"
  },
  "lock": false,
  "fmt": {
    "useTabs": false,
    "lineWidth": 80,
    "indentWidth": 2,
    "singleQuote": true
  },
  "name": "@goatdb/goatdb",
  "version": "0.0.49",
  "license": "AGPL/ELV2",
  "exports": {
    ".": "./mod.ts",
    "./react": "./react/hooks.ts",
    "./server": "./server/mod.ts",
    "./init": "./cli/init.ts"
  },
  "tasks": {
    "build": "deno run -A packaging/build-sys-assets.ts"
  },
  "imports": {
    "@aws-sdk/client-sesv2": "npm:@aws-sdk/client-sesv2@^3.726.1",
    "@luca/esbuild-deno-loader": "jsr:@luca/esbuild-deno-loader@^0.11.1",
    "@std/data-structures": "jsr:@std/data-structures@^1.0.6",
    "@std/fs": "jsr:@std/fs@^1.0.9",
    "@std/path": "jsr:@std/path@^1.0.8",
    "@std/encoding": "jsr:@std/encoding@1.0.6",
    "esbuild": "npm:esbuild@^0.24.2",
    "react": "npm:react@19.0.0",
    "react-jss": "npm:react-jss@10.10.0",
    "yargs": "npm:yargs@17.7.2"
  }
}
